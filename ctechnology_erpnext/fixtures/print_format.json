  "html": "<style>\n\t.print-format table, .print-format tr, \n\t.print-format td, .print-format div, .print-format p {\n\t\tfont-family: Verdana;\n\t\tline-height: 150%;\n\t\tfont-size: 13px;\n\t\tvertical-align: middle;\n\t}\n\t.col-xs-2, .col-xs-3, .col-xs-4, .col-xs-8 {\n\t\twhite-space: nowrap;\n\t}\n\t@media screen {\n\t\t.print-format {\n\t\t\twidth: 21.97cm;\n\t\t\theight: 27.94cm;\n\t\t}\n\t}\n</style>\n\n{% set item_count = doc.items|length %}\n{% set page_count = (item_count / 17)|round(0, 'ceil') %}\n\n{% set header_html %}\n<!-- BRANCH & Transaction Type -->\n\t<div class=\"row\" style=\"margin-top: 1.5in\">\n        <div class=\"col-xs-6 text-right\">\n          <b><p style=\"font-size: 13px; text-decoration: underline;\">{{ doc.territory }}</p></b>\n        </div>\n        <div class=\"col-xs-6 text-left\">\n          <b><p style=\"font-size: 13px; text-decoration: underline;\">\n\t\t{% if doc.is_pos %}\n\t\t\tCash Invoice\n\t\t{% else %}\n\t\t\tCredit Invoice\n\t\t{% endif %}\t\n\t\t</p></b>\n        </div>\n  \t</div>\n  \n\t<!-- CUSTOMER CODE & DATE -->\n    <div class=\"row\" style=\"margin-top:20px\">\n    \t<div class=\"col-xs-2 text-left\">Customer Code</div>\n        <div class=\"col-xs-5 text-left\">:  {{ doc.customer }}</div>\n        <div class=\"col-xs-2 text-left\">&emsp;&emsp;&emsp;Date</div>\n        <div class=\"col-xs-3 text-left\">:  {{ doc.get_formatted(\"posting_date\") }}</div>\n    </div>\n  \n  \n  <!-- CUSTOMER NAME & INVOICE NO -->\n    <div class=\"row\">\n    \t<div class=\"col-xs-2 text-left\">Customer Name</div>\n        <div class=\"col-xs-5 text-left\">:  {{ doc.customer_name }}</div>\n        <div class=\"col-xs-2 text-left\">&emsp;&emsp;&emsp;Invoice No</div>\n        <div class=\"col-xs-3 text-left\">:  {{ doc.name }}</div>\n    </div>\n    \n    \n    <!-- Contact Person & Page NO -->\n    <div class=\"row\">\n    \t<div class=\"col-xs-2 text-left\">Contact Person</div>\n        <div class=\"col-xs-5 text-left\">:  _contatct_</div>\n        <div class=\"col-xs-2 text-left\">&emsp;&emsp;&emsp;Page No</div>\n        <div class=\"col-xs-3 text-left\">:  page_no / page_count</div>\n    </div>\n    \n    \n    <!-- Address & Salesman ID -->\n    <div class=\"row\">\n    \t<div class=\"col-xs-2 text-left\">Address</div>\n        <div class=\"col-xs-5 text-left\">:  _address_</div>\n        <div class=\"col-xs-2 text-left\">&emsp;&emsp;&emsp;Salesman ID</div>\n        <div class=\"col-xs-3 text-left\">:  SP1001</div>\n    </div>\n    \n    <!-- Tele No & Salesman -->\n    <div class=\"row\">\n    \t<div class=\"col-xs-2 text-left\">Tele No.</div>\n        <div class=\"col-xs-5 text-left\">:  _tele_</div>\n        <div class=\"col-xs-2 text-left\">&emsp;&emsp;&emsp;Salesman</div>\n        <div class=\"col-xs-3 text-left\">:  MOHMMED NASSER</div>\n    </div>\n    \n    <!-- Fax No & Reference -->\n    <div class=\"row\">\n    \t<div class=\"col-xs-2 text-left\">Fax No.</div>\n        <div class=\"col-xs-5 text-left\">:  _fax_</div>\n        <div class=\"col-xs-2 text-left\">&emsp;&emsp;&emsp;Reference</div>\n        <div class=\"col-xs-3 text-left\">:  #25336</div>\n    </div>\n    \n    <!-- Description -->\n    <div class=\"row\">\n    \t<div class=\"col-xs-2 text-left\">Description</div>\n        <div class=\"col-xs-10 text-left\">:  _description_</div>\n    </div>\n    <hr style=\"margin: 10px 0px 0px 0px;\">\n{% endset %}\n    \n{% set table_html1 %}\n    <div class=\"table-borderless items-table\" style=\"height: 4.5in\">\n        <table class=\"table\" style=\"margin: 0px;\">\n            <!-- COLUMNS -->\n            <tbody>\n                <tr style=\"font-size: 20px;\">\n                    <td width=\"20%\">Item Code</td>\n                    <td width=\"40%\">Item Description</td>\n                    <td width=\"10%\">Unit</td>\n                    <td width=\"5%\">Qty</td>\n                    <td style=\"white-space: nowrap;\" width=\"15%\"><div class=\"pull-right\">Unit Price</div></td>\n                    <td width=\"10%\"><div class=\"pull-right\">Total</div></td>\n                </tr>\n            </tbody>\n            <tbody>\n\n{% endset %}\n{% set table_html2 %}\n            </tbody>\n        </table>\n    </div>\n{% endset %}\n\n\n{% set item_row_html %}\n\t<tr>\n    \t\t<td style=\"font-size: 12px; border-top: none;\">code</td>\n    \t\t<td style=\"font-size: 12px; border-top: none;\">name</td>\n    \t\t<td style=\"font-size: 12px; border-top: none;\">unit</td>\n    \t\t<td style=\"font-size: 12px; border-top: none;\">qty</td>\n\t\t\t<td style=\"font-size: 12px; border-top: none;\"><div class=\"pull-right\">price</div></td>\n    \t\t<td style=\"font-size: 12px; border-top: none;\"><div class=\"pull-right\">total</div></td>\n    \t</tr>\n{% endset %}\n\n{% set footer_html %}    \n    <div class=\"row\" style=\"margin: 0px 0px 0px 0px;\">\n        <div class=\"row\">\n            <div class=\"col-xs-8\">\n            \t<div class=\"row\" style=\"font-size: 10px; margin-left: 20px;\">\n\t                Note: Items returned after 3 days from the invoice date will not be taken back.\n                </div>\n                <div class=\"row\" style=\"font-size: 10px; margin: 20px 0px 0px 20px\">{{ doc.in_words }}</div>\n            </div>\n            <div class=\"col-xs-4\">\n            \t<div class=\"row\">\n                    <div class=\"col-xs-4\">Gross Amt</div>\n                    <div class=\"col-xs-7\">&emsp;: <div class=\"pull-right\">{{ doc.get_formatted(\"total\") }}</div></div>\n                    <div class=\"col-xs-4\">Discount</div>\n                    <div class=\"col-xs-7\">&emsp;: <div class=\"pull-right\">{{ doc.get_formatted(\"discount_amount\") }}</div></div>\n                    <div class=\"col-xs-4\">Other Charge</div>\n                    <div class=\"col-xs-7\">&emsp;: <div class=\"pull-right\">0.00</div></div>\n                    <div class=\"col-xs-4\">Net Amount</div>\n                    <div class=\"col-xs-7\">&emsp;: <div class=\"pull-right\">{{ doc.get_formatted(\"rounded_total\") }}</div></div>                \n                </div>\n            </div>\n        </div>\n    </div>\n{% endset %} \n\n{% for i in range(page_count|int()) %}\n\t{{ header_html|replace(\"page_no\", i + 1)|replace(\"page_count\", page_count|int()) }}\n\t\n\t{% set range_stop = (i + 1) * 15 %}\n\t{% if range_stop > doc.items|length %}\n\t\t{% set range_stop = doc.items|length %}\n\t{% endif %}\n\t{{ table_html1 }}\t\t\n\t{% for j in range(i * 15, range_stop|int(), 1)  %}\n\t\t{% set item = doc.items[j] %}\n\t\t{{ item_row_html|replace(\"code\", item.item_code)|replace(\"name\", item.description)|replace(\"qty\", item.qty)|replace(\"unit\", item.stock_uom)|replace(\"price\", item.get_formatted(\"rate\"))|replace(\"total\", item.get_formatted(\"amount\")) }}\t\t\n\t{% endfor %}\n\t{{ table_html2 }}\n\t\n\t{% if i < page_count|int() - 1 %}\n\t\t<div class=\"page-break\"></div>\n\t{% endif %}\n{% endfor %}\n\n{{ footer_html }}\n", 
